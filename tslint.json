{
  "extends": "../../tslint.json",
  "rules": {
    "directive-selector": [
      true,
      "attribute",
      "ngx",
      "camelCase"
    ],
    "component-selector": [
      true,
      "element",
      "ngx",
      "kebab-case"
    ],
    "no-string-literal": false,
    "no-output-rename": false,
    "no-output-on-prefix": false,
    "no-output-native": false,
    "no-input-rename": false,
    "ban-types": false
  }
}
